      "DriverLicense": {
        "description": "Водительские права",
        "required": [
          "id",
          "type",
          "series",
          "number",
          "issueDate"
        ],
        "allOf": [
          {
            "$ref": "#/components/schemas/Document"
          },
          {
            "properties": {
              "series": {
                "type": "string",
