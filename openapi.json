          "date",
          "isPartTimeJob",
          "organization"
        ],
        "type": "object",
        "allOf": [
          {
            "$ref": "#/components/schemas/ElectronicWorkbookEntry"
          },
          {
            "properties": {
              "position": {
                "type": "string",
                "description": "Название позиции",
                "nullable": true
              }
            }
          }
        ]
      },
      "ElectronicWorkbookDismissalEntry": {
